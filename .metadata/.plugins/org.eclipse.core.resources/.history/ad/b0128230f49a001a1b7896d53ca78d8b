package primerArray;

public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Ejemplo arreglo de Strings
		
		String nombres[];
		nombres = new String[3];
		
		nombres[0] = "Chuchita";
		nombres[1] = "Aureliano";
		nombres[2] = "Ursula";
		
		System.out.println(nombres[0]);
		System.out.println(nombres[1]);
		System.out.println(nombres[2]);
	}

}
