package main;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Usuario Jerry = new Usuario("Jerry");
		Usuario Juan = new Usuario("Juan", 12);
		Usuario Remedios = new Usuario("Remedios", "Macondo");
		
